{
	"reporterEnabled": "mocha-junit-reporter, mochawesome",
	"mochaJunitReporterReporterOptions": {
		"mochaFile": "cypress/tests_results/junit/chrome/results-[hash].xml",
		"toConsole": false,
		"outputs": true,
		"testCaseSwitchClassnameAndName": true
	},
	"reporterOptions": {
		"reportDir": "cypress/tests_results/mocha/chrome",
		"overwrite": true,
		"html": true,
		"json": true
	}
}
